module cosrom (
    input  [7:0] addr,           // 0-90 valid
    output reg [15:0] data
);

    reg [15:0] mem [0:90];       // 91 entries

    initial begin
        mem[0]  = 16'b1000000000000000;
        mem[1]  = 16'b0111111111111011;
        mem[2]  = 16'b0111111111101100;
        mem[3]  = 16'b0111111111010011;
        mem[4]  = 16'b0111111110110000;
        mem[5]  = 16'b0111111110000011;
        mem[6]  = 16'b0111111101001100;
        mem[7]  = 16'b0111111100001011;
        mem[8]  = 16'b0111111011000001;
        mem[9]  = 16'b0111111001101100;
        mem[10] = 16'b0111111000001110;
        mem[11] = 16'b0111110110100101;
        mem[12] = 16'b0111110100110011;
        mem[13] = 16'b0111110010111000;
        mem[14] = 16'b0111110000110010;
        mem[15] = 16'b0111101110100011;
        mem[16] = 16'b0111101100001010;
        mem[17] = 16'b0111101001101000;
        mem[18] = 16'b0111100110111100;
        mem[19] = 16'b0111100100000110;
        mem[20] = 16'b0111100001000111;
        mem[21] = 16'b0111011101111111;
        mem[22] = 16'b0111011010101101;
        mem[23] = 16'b0111010111010011;
        mem[24] = 16'b0111010011101111;
        mem[25] = 16'b0111010000000001;
        mem[26] = 16'b0111001100001011;
        mem[27] = 16'b0111001000001100;
        mem[28] = 16'b0111000100000100;
        mem[29] = 16'b0110111111110011;
        mem[30] = 16'b0110111011011001;
        mem[31] = 16'b0110110110110111;
        mem[32] = 16'b0110110010001100;
        mem[33] = 16'b0110101101011001;
        mem[34] = 16'b0110101000011101;
        mem[35] = 16'b0110100011011001;
        mem[36] = 16'b0110011110001101;
        mem[37] = 16'b0110011000111001;
        mem[38] = 16'b0110010011011101;
        mem[39] = 16'b0110001101111001;
        mem[40] = 16'b0110001000001101;
        mem[41] = 16'b0110000010011010;
        mem[42] = 16'b0101111100011111;
        mem[43] = 16'b0101110110011100;
        mem[44] = 16'b0101110000010011;
        mem[45] = 16'b0101101010000010;
        mem[46] = 16'b0101100011101010;
        mem[47] = 16'b0101011101001011;
        mem[48] = 16'b0101010110100110;
        mem[49] = 16'b0101001111111001;
        mem[50] = 16'b0101001001000110;
        mem[51] = 16'b0101000010001101;
        mem[52] = 16'b0100111011001101;
        mem[53] = 16'b0100110100001000;
        mem[54] = 16'b0100101100111100;
        mem[55] = 16'b0100100101101010;
        mem[56] = 16'b0100011110010011;
        mem[57] = 16'b0100010110110110;
        mem[58] = 16'b0100001111010100;
        mem[59] = 16'b0100000111101100;
        mem[60] = 16'b0100000000000000;
        mem[61] = 16'b0011111000001110;
        mem[62] = 16'b0011110000010111;
        mem[63] = 16'b0011101000011100;
        mem[64] = 16'b0011100000011100;
        mem[65] = 16'b0011011000011000;
        mem[66] = 16'b0011010000001111;
        mem[67] = 16'b0011001000000011;
        mem[68] = 16'b0010111111110011;
        mem[69] = 16'b0010110111011111;
        mem[70] = 16'b0010101111000111;
        mem[71] = 16'b0010100110101100;
        mem[72] = 16'b0010011110001101;
        mem[73] = 16'b0010010101101100;
        mem[74] = 16'b0010001101001000;
        mem[75] = 16'b0010000100100000;
        mem[76] = 16'b0001111011110111;
        mem[77] = 16'b0001110011001011;
        mem[78] = 16'b0001101010011100;
        mem[79] = 16'b0001100001101100;
        mem[80] = 16'b0001011000111010;
        mem[81] = 16'b0001010000000110;
        mem[82] = 16'b0001000111010000;
        mem[83] = 16'b0000111110011001;
        mem[84] = 16'b0000110101100001;
        mem[85] = 16'b0000101100100111;
        mem[86] = 16'b0000100011101101;
        mem[87] = 16'b0000011010110010;
        mem[88] = 16'b0000010001110111;
        mem[89] = 16'b0000001000111011;
        mem[90] = 16'b0000000000000000;
    end

    always @(*) begin
       if(addr<=8'd90)
        data = mem[addr];
        else
        data=16'h0000;
    end

endmodule




